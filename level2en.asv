function[r1,r2,r3]=level2en(F)
% This function conducts the level two encoding of the input infomation
% polynomial F(x)
% c11(x)=[1,3,2];
% c12(x)=[1,7,12];
% c13(x)=[1,11,30];
c11=[1,3,2];
c12=[1,7,12];
c13=[1,11,30];
[q,r1]=decon(F,c11);
[q,r2]=decon(F,c12);
[q,r3]=decon(F,c13);
turn
end